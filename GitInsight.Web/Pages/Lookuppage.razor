@page "/lookuppage/{User?}/{Repo?}"
@using GitInsight;

@code {
    [Parameter]
    public string? User { get; set; } = "";
    
    [Parameter]
    public string? Repo{ get; set; } = "";
    
    protected override void OnInitialized()
    {
        User = User ?? "";
        Repo = Repo ?? "";
        GetRepo();
    }
    public void GetRepo(){
        var lookup = new Lookup();
        string repopath = (User + "/" + Repo);
        lookup.CheckRepo(repopath, Repo);
    }    
}
